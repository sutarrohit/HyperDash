{
    "$schema": "https://turborepo.dev/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "env": ["DATABASE_URL"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [".next/**", "dist/**", "!.next/cache/**"]
        },
        "lint": {
            "dependsOn": ["^lint"]
        },
        "check-types": {
            "dependsOn": ["^check-types"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "inputs": ["$TURBO_DEFAULT$", ".env*", "tsconfig.json"]
        },
        "test": {
            "cache": false
        },
        "db:migrate:dev": {
            "cache": false
        },
        "db:migrate:deploy": {
            "cache": false
        },
        "db:push": {
            "cache": false
        },
        "db:seed": {
            "cache": false
        },
        "db:generate": {
            "cache": false
        },
        "db:studio": {
            "cache": false,
            "persistent": true
        },
        "generate": {
            "dependsOn": ["^generate"],
            "cache": false
        }
    }
}
